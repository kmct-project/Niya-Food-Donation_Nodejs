<div class="page-wrapper mb-4">

    <div class="page-breadcrumb mb-5">

<div class="container mt-4 mb-4">
        <div class="row">
            <div class="col-md-6 mx-auto">
                <h2 class="text-center">Edit Time</h2>
                <form action="/foodspots/edit-time/{{time._id}}" enctype="multipart/form-data" method="POST">
                    <label for="" class="mt-3">From</label>
                    <input type="time" name="from" class="form-control" value="{{time.from}}" required>
                
                    <label for="" class="mt-3">To</label>
                    <input type="time" name="to" class="form-control" value="{{time.to}}" required>
                
                    <button type="submit" class="btn btn-primary mb-5 w-100 mt-4">Submit</button>
                    <br>
                </form>
            </div>
        </div>
    </div>

    </div>
